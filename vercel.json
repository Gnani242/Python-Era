{
    "version": 2,
    "builds": [
        {
            "src": "python-era/backend/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "python-era/frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/python-era/backend/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/python-era/frontend/$1",
            "headers": {
                "cache-control": "s-maxage=0"
            }
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/python-era/frontend/index.html"
        }
    ]
}